{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/app.py"
        }
    ],
    "env": {
        "FLASK_ENV": "production"
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/app.py"
        }
    ],
    "alias": "karandixitportfolio.vercel.app",
    "framework": "flask"
}